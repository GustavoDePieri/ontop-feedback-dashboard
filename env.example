# Environment variables for Ontop Feedback Dashboard
# Copy this file to .env and update values as needed

# Port for the application (Render will set this automatically)
PORT=8501

# Python version
PYTHON_VERSION=3.9.18

# Streamlit configuration
STREAMLIT_SERVER_HEADLESS=true
STREAMLIT_SERVER_ENABLE_CORS=false
STREAMLIT_SERVER_ENABLE_XSRF_PROTECTION=false
STREAMLIT_BROWSER_GATHER_USAGE_STATS=false

# Data file configuration (optional - defaults to auto-detect)
# CSV_FILE_PATH=clientsFeedbackWithArea_utf8.csv
